{
  "metadata": {
    "name": "Testing",
    "description": "Mutation for application testing",
    "image": {
      "url": "https://raw.githubusercontent.com/dapplets/bos-components/master/src/assets/Testing.svg"
    }
  },
  "$apps": [
    "${REPL_ACCOUNT}/app/OnboardingTest",
    "${REPL_ACCOUNT}/app/Capybara",
    "${REPL_ACCOUNT}/app/CapybaraTestMWeb",
    "${REPL_ACCOUNT}/app/CrossPost",
    "${REPL_ACCOUNT}/app/AigencyCrawler",
    "${REPL_ACCOUNT}/app/Xen",
    "${REPL_ACCOUNT}/app/CapybaraEverywhere"
  ],
  "$targets": [
    {
      "namespace": "engine",
      "contextType": "website",
      "if": {
        "id": {
          "in": [
            "x.com",
            "twitter.com",
            "github.com",
            "youtube.com",
            "www.youtube.com",
            "linkedin.com",
            "www.linkedin.com",
            "reddit.com",
            "www.reddit.com",
            "social.dapplets.org",
            "localhost",
            "127.0.0.1",
            "social.testnet.dapplets.org",
            "app.near.ai"
          ]
        }
      }
    },
    {
      "namespace": "engine",
      "contextType": "website",
      "if": {
        "id": {
          "contains": "social.testnet.apps.dapplets.org"
        }
      }
    }
  ]
}
