{
  "metadata": {
    "name": "Onboarding",
    "description": "Onboarding for the mutation",
    "image": {
      "url": "https://raw.githubusercontent.com/dapplets/bos-components/master/src/assets/Desktop.svg"
    }
  },
  "$targets": [
    {
      "namespace": "${REPL_ACCOUNT}/parser/github-onboarding",
      "contextType": "body",
      "if": { "id": { "not": null, "index": true } },
      "injectTo": "end",
      "injectOnce": true,
      "componentId": "${REPL_ACCOUNT}/widget/Onboarding.Main"
    },
    {
      "namespace": "${REPL_ACCOUNT}/parser/linkedin-onboarding",
      "contextType": "body",
      "if": { "id": { "not": null, "index": true } },
      "injectTo": "end",
      "injectOnce": true,
      "componentId": "${REPL_ACCOUNT}/widget/Onboarding.Main"
    }
  ]
}
