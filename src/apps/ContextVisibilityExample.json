{
  "metadata": {
    "name": "Context Visibility App Example"
  },
  "$": {
    "controller": "${REPL_ACCOUNT}/widget/ContextVisibilityExample.Main",
    "targets": [
      {
        "namespace": "${REPL_ACCOUNT}/parser/twitter",
        "contextType": "post",
        "isVisible": true,
        "if": { "id": { "not": null } },
        "limit": 1,
        "injectTo": "southPanel",
        "componentId": "${REPL_ACCOUNT}/widget/ContextVisibilityExample.Main",
        "static": true
      }
    ]
  }
}
