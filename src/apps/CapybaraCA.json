{
  "metadata": {
    "name": "Capybara Connected Accounts",
    "description": "A button that displays the connected accounts",
    "image": {
      "url": "https://raw.githubusercontent.com/dapplets/bos-components/master/src/assets/Capybara.svg"
    }
  },
  "$targets": [
    {
      "namespace": "${REPL_ACCOUNT}/parser/near-social",
      "contextType": "post",
      "if": { "id": { "not": null, "index": true } },
      "injectTo": "root",
      "componentId": "${REPL_ACCOUNT}/widget/CapybaraBtnCA"
    },
    {
      "namespace": "${REPL_ACCOUNT}/parser/twitter",
      "contextType": "post",
      "if": { "id": { "not": null, "index": true } },
      "injectTo": "root",
      "componentId": "${REPL_ACCOUNT}/widget/CapybaraBtnCA"
    },
    {
      "namespace": "${REPL_ACCOUNT}/parser/github",
      "contextType": "post",
      "if": { "id": { "not": null, "index": true } },
      "injectTo": "root",
      "componentId": "${REPL_ACCOUNT}/widget/CapybaraBtnCA"
    },
    {
      "namespace": "${REPL_ACCOUNT}/parser/youtube",
      "contextType": "post",
      "if": { "id": { "not": null, "index": true } },
      "injectTo": "root",
      "componentId": "${REPL_ACCOUNT}/widget/CapybaraBtnCA"
    },
    {
      "namespace": "${REPL_ACCOUNT}/parser/linkedin",
      "contextType": "post",
      "if": { "id": { "not": null, "index": true } },
      "injectTo": "root",
      "componentId": "${REPL_ACCOUNT}/widget/CapybaraBtnCA"
    },
    {
      "namespace": "${REPL_ACCOUNT}/parser/reddit-new",
      "contextType": "post",
      "if": { "id": { "not": null, "index": true } },
      "injectTo": "root",
      "componentId": "${REPL_ACCOUNT}/widget/CapybaraBtnCA"
    }
  ]
}
